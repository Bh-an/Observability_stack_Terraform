global:
  scrape_interval: 15s
  evaluation_interval: 15s
  external_labels:
    cluster: 'prometheus-ha-prod'
    replica: '__PROMETHEUS_REPLICA_ID__'

rule_files:

scrape_configs:
  - job_name: "prometheus"
    static_configs:
      - targets: ["localhost:9090"]


    